# Mailman completion by Guillaume Rousse <rousse@ccr.jussieu.fr>


comp_include _get_cword _mailman_lists


_list_owners()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W '-w --with-listnames \
			-m --moderators -h --help' -- $cur ) )
	else
		_mailman_lists
	fi
} # _list_owners()
