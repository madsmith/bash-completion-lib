proc setup {} {
}; # setup()


proc teardown {} {
}; # teardown()


set test "Ant should be available"
if {[test_command {ant --help} $test result]} {


    setup


    set test "Tab should complete"
	# Try completion
    set cmd "ant -"
    send "$cmd\t"
    expect {
	-re "^$cmd\r\n.*\r\n/@$cmd$" { pass "$test" }
	-re /@ { unresolved "$test at prompt" }
	-re eof { unresolved "eof" }
    }; # expect


    sync_after_int


    teardown


}; # if


