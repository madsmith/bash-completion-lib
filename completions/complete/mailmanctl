# Mailman completion by Guillaume Rousse <rousse@ccr.jussieu.fr>
#

_mailmanctl()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W '-n --no-restart -u --run-as-user \
			-s --stale-lock-cleanup --quiet -q -h --help' -- $cur ) )
	else
		COMPREPLY=( $( compgen -W 'start stop restart reopen' -- $cur ) )
	fi
} # _mailmanctl()


