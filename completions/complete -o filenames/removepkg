# Slackware Linux removepkg completion
#
_removepkg()
{
    local packages cur

    COMPREPLY=()
    cur=`_get_cword`

    COMPREPLY=( $( (cd /var/log/packages && compgen -f -- "$cur") ) )
} # _removepkg()

comp_install  # Make completion persistent for subsequent invocations

_removepkg  # Generate completions for first invocation
