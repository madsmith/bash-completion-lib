# Mailman completion by Guillaume Rousse <rousse@ccr.jussieu.fr>
#

_dumpdb()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W '--marshal -m --pickle -p --noprint -n -h --help' -- $cur ) )
	else
		_filedir
	fi
} # _dumpdb()

comp_install  # Make completion persistent for subsequent invocations

_dumpdb  # Generate completions for first invocation
