# tcpnice(8) completion
#
_tcpnice()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`
	prev=${COMP_WORDS[COMP_CWORD-1]}

	case "$prev" in
		-i)
			_interfaces
			return 0
			;;
	esac

	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W '-A -I -M -i' -- $cur ) )
	fi
} # _tcpnice()

comp_install  # Make completion persistent for subsequent invocations

_tcpnice  # Generate completions for first invocation
