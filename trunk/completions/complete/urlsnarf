# urlsnarf(8) completion
#
_urlsnarf()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`
	prev=${COMP_WORDS[COMP_CWORD-1]}

	case "$prev" in
		-i)
			_interfaces
			return 0
			;;
	esac

	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W '-n -i -v' -- $cur ) )
	fi
} # _urlsnarf()

comp_install  # Make completion persistent for subsequent invocations

_urlsnarf  # Generate completions for first invocation
