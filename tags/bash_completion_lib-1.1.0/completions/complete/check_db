# Mailman completion by Guillaume Rousse <rousse@ccr.jussieu.fr>
#

comp_include _mailman_lists

_check_db()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W '--all -a --verbose -v \
			-h --help' -- $cur ) )
	else
		_mailman_lists
	fi
} # _check_db()

comp_install  # Make completion persistent for subsequent invocations

_check_db  # Generate completions for first invocation
