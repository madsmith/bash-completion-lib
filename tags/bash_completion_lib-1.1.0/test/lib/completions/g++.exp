proc setup {} {
}; # setup()


proc teardown {} {
}; # teardown()


setup


set test "Tab should complete"
    # Try completion
send "g++ \t"
expect {
    -re "^g\\+\\+ .*$" { pass "$test" }
    -re /@ { unresolved "$test at prompt" }
    -re eof { unresolved "eof" }
}; # expect


# Interrupt command by sending ^C

sleep 0.3; 
send \003;  # Control-c
expect -ex /@


teardown
