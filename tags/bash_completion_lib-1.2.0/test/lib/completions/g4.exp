proc setup {} {
}; # setup()


proc teardown {} {
}; # teardown()


setup


set test "Tab should complete"
    # Try completion
set cmd "g4 "
send "$cmd\t"
expect {
    -re "^${cmd}(bash: p4: command not found\r\n)\r\n.*$cmd$" { pass "$test" }
    -re /@ { unresolved "$test at prompt" }
    -re eof { unresolved "eof" }
}; # expect


# Interrupt command by sending ^C

send \003;  # Control-c
send "\n"
expect -ex "/@\r\n/@"


teardown
