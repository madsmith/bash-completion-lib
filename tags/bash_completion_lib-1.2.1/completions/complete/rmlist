# Mailman completion by Guillaume Rousse <rousse@ccr.jussieu.fr>
#

comp_include _mailman_lists

_rmlist()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W '--archives -a \
			-h --help' -- $cur ) )
	else
		_mailman_lists
	fi
} # _rmlist()

comp_install  # Make completion persistent for subsequent invocations

_rmlist  # Generate completions for first invocation
