# Linux cardctl(8) completion
#
_cardctl()
{
    local cur

    COMPREPLY=()
    cur=`_get_cword`

    if [ $COMP_CWORD -eq 1 ]; then
        COMPREPLY=( $( compgen -W 'status config ident suspend \
                       resume reset eject insert scheme' \
                   -- $cur ) )
    fi
} # _cardctl()


