# Mailman completion by Guillaume Rousse <rousse@ccr.jussieu.fr>
#

comp_include _mailman_lists

_cleanarch()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W '-s --status -n --dry-run \
			-q --quiet -h --help' -- $cur ) )
	fi
} # _cleanarch()


