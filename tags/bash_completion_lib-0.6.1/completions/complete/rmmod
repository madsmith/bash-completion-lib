# Linux rmmod(8) completion. This completes on a list of all currently
# installed kernel modules.
_rmmod()
{
    local cur

    COMPREPLY=()
    cur=${COMP_WORDS[COMP_CWORD]}

    COMPREPLY=( $( /sbin/lsmod | \
          awk '{if (NR != 1 && $1 ~ /^'$cur'/) print $1}' 2>/dev/null ))
    return 0
}
_rmmod
